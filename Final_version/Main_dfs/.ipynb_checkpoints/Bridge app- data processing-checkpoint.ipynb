   "source": [
    "## Creating data frames in English"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "metadata": {},
   "outputs": [],
   "source": [
    "def english_data_frame(history,stats,scores):\n",
    "    scores_eng=scores.copy()\n",
    "    history_eng=history.copy()\n",
    "    stats_eng=stats.copy()\n",
    "    \n",
    "    history_eng['zapis2']=history_eng['zapis2'].replace({'końcówka':'game', 'częściówka':'partial', 'szlemik':'slame or above', 'Kokosy_na_Obronie':'defence_profit', 'Wpadka':'defeat'})\n",
    "    \n",
    "    scores_eng.columns=['place','pair_id','score','tournament_number','name','WK','PID']\n",
    "    history_eng.columns=['id','played_by','score_%_board','board_number','tournament_number','score_points','line','place','number','score_%_turnament','name','WK','PID','is_declarer','contract_type']\n",
    "    stats_eng.columns=['PID','avg_declarer','avg_defence','defence_profit','partial','game','slame or above','defeat','name']\n",
    "    \n",
    "    return history_eng,stats_eng,scores_eng"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "metadata": {},
   "outputs": [],
   "source": [
    "history_eng,stats_eng,scores_eng=english_data_frame(history,stats,scores)"
   ]
  },
