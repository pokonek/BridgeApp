  },
  {
   "cell_type": "code",
   "execution_count": 13,
   "metadata": {},
   "outputs": [],
   "source": [
    "def five_zeros(df):\n",
    "    wektor=[]\n",
    "    for v in df['PID']:\n",
    "        wektor.append(v.rjust(5,'0'))\n",
    "    df['PID']=wektor\n",
    "    return df"
